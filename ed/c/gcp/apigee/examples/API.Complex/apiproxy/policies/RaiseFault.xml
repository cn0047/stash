<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RaiseFault">
    <DisplayName>RaiseFault</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="application/json">{"message":"Forbidden", "path": "{proxy.pathsuffix}"}</Payload>
            <StatusCode>403</StatusCode>
            <ReasonPhrase>FORBIDDEN</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
